PARAMETERS FILE EXAMPLES
(CTRL-F and change "./" to the desired base path.)


--------------------------------------------------------- BIKE
---- HT & PARALLEL HT
run_for='single_image_test'
HT_version='HT'
parallel=true
parallel_type='openMP'
thread_count=100
input='./dataset/images/bike.jpg'
output_folder='./output/test/'
performance_path='./performance'
convert_output=true
converter_program_location='./src/python/image_converter.py'
conversion_format='jpg'
verbose=true
greyscale_conversion=true
gaussian_blur=true
gb_kernel_size=7
gb_sigma=1.9
histogram_equalization=true
sobel_edge_detection=true
sed_threshold=160
sed_scale_factor=1
hough_vote_threshold=120
hough_theta=240
cluster_similar_lines=true
cluster_theta_threshold=2.0
cluster_rho_threshold=5.0
sampling_rate=100
ppht_line_gap=10
ppht_line_len=30
detection_distance_threshold=5

